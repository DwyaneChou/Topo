include ../../machine_settings.make
#
# LD_LIBRARY_PATH= is needed to make Python work
#
#LD_LIBRARY_PATH=
#
# MODIS specific
#
field=01

all: landwater.nc
#
# even though we need all tif files listed below, just one rule target used here
#
rawdata/DEM_SN_A.h15v02.006_0.hdf_06.tif:
	for f in $(wildcard rawdata/*.hdf); do \
		$(python_path)/gdal_translate -sds $$f $$f.tif; \
	done;

$(field).all.tif: rawdata/DEM_SN_A.h15v02.006_0.hdf_06.tif
	$(python_path)/python $(python_path)/gdal_merge.py $(tif_files) -o $(field).all.tif


landwater.nc: $(field).all.tif
	echo "cccc $(python_path)"
	$(python_path)/gdalwarp -of netCDF -t_srs "EPSG:4326" $(field).all.tif landwater.nc

clean:
	rm rawdata/*.tif
	rm $(field).all.tif

#
# $(wildcard rawdata/*.hdf_$field.tif)

